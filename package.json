{
  "name": "@bsd/overlay",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "example": "open ./example/index.html",
    "doc": "jsdoc -c jsdoc.conf.json -d docs -u src --readme README.md",
    "doc:open": "open ./docs/index.html",
    "postinstall": "cd host && yarn",
    "host": "./host/start.sh",
    "dev": "concurrently \"yarn run watch\" \"yarn run host\""
  },
  "dependencies": {},
  "devDependencies": {
    "@markvl/jsdoc-template": "3.2.0",
    "concurrently": "4.0.1",
    "jsdoc": "3.5.5",
    "postcss-color-function": "4.0.1",
    "postcss-nested": "3.0.0",
    "postcss-variables": "1.1.1",
    "rollup": "0.65.2",
    "rollup-plugin-commonjs": "9.1.6",
    "rollup-plugin-eslint": "5.0.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-postcss-modules": "1.0.8",
    "rollup-plugin-typescript2": "0.17.0",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "3.0.3"
  }
}
