{
  "source": {
    "include": ["src/"]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "referenceTitle": "BSD Overlay JS API",
    "disableSort": false,
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "includeDate": false,
      "staticFiles": {
        "include": ["images"]
      }
    }
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/@markvl/jsdoc-template"
  }
}